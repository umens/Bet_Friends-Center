    "connection": {
      "port": 80,
      "routes": {
        "cors": {
          "origin": [
            "*"
          ],
          "headers": [
            "Access-Control-Allow-Origin",
            "Access-Control-Allow-Headers",
            "Origin",
            "X-Requested-With",
            "Content-Type",
            "Authorization"
          ],
          "credentials": true
